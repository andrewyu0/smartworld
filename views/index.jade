  script(type='text/javascript', src='./javascripts/otto.js')

  img(src='./images/bulb-diffuser.png', style='display: none;')
  img(src='./images/bulb-highlight.png', style='display: none;')
  img(src='./images/bulb-shadow.png', style='display: none;')
  script(type='text/javascript', src='https://cdn.firebase.com/js/client/1.0.6/firebase.js')
  script(type='text/javascript').
    var ottoSWLightRef = new Firebase('https://otto.firebaseio.com/'+ "#{loc}");
    ottoSWLightRef.on('value', function(light) {
      var hsv = light.val();
      var lightState = {
        brightness: hsv.b,
        duration: 250,
        hue: hsv.h,
        kelvin: 3500,
        power: hsv.on,
        saturation: hsv.s
      };
      colorChange((360*lightState.hue).toString(),(100*lightState.saturation).toString(),!lightState.power?"0":(100*lightState.brightness).toString(),lightState.kelvin.toString(),lightState.duration.toString())
    });
